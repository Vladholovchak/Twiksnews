<h2>Sources</h2>
<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th colspan="10"></th>
    <th colspan="10"></th>
  </tr>

  <tr>
    <%  @sources.each do |source| %>
      <td><%= source.google_id %></td>
      <td><%= source.name %></td>
      <td><%= link_to 'Show', source_path(source) %></td>
      <td> <%= link_to 'Edit', edit_source_path(source) %></td>
      <td><%= link_to 'Destroy', source_path(source),
                      method: :delete,
                      data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <%end %>

    </table>